{
  "name": "project19-web",
  "version": "0.0.1",
  "adonis-version": "4.1.0",
  "description": "Project19 web server.",
  "scripts": {
    "start": "cross-env ENV_PATH=\"../.env\" adonis serve",
    "nodemon": "cross-env ENV_PATH=\"../.env\" nodemon",
    "adonis": "cross-env ENV_PATH=\"../.env\" adonis",
    "migration": "cross-env ENV_PATH=\"../.env\" node ace migration:run",
    "css-build": "node-sass resources/sass/mystyles.scss public/css/stylesheet.css --output-style compressed",
    "css-build-plain": "node-sass resources/sass/plain.scss public/css/stylesheet-plain.css --output-style compressed"
  },
  "keywords": [
    "project19",
    "web-app"
  ],
  "author": "Karar Al-Remahy",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/bodyparser": "^2.0.9",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.13",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/websocket": "^1.0.12",
    "feather-icons": "^4.25.0",
    "node-fetch": "^2.6.0",
    "pg": "^7.14.0"
  },
  "autoload": {
    "App": "./app"
  },
  "devDependencies": {
    "bulma": "^0.8.0",
    "node-sass": "^4.13.0"
  }
}
